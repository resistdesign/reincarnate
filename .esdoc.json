{
  "source": "./src",
  "destination": "./docs",
  "includes": [
    "\\.jsx$"
  ],
  "excludes": [
    "\\.spec\\.jsx$"
  ],
  "plugins": [
    {
      "name": "esdoc-standard-plugin"
    },
    {
      "name": "esdoc-jsx-plugin",
      "option": {
        "enable": true
      }
    },
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": {
        "all": true
      }
    }
  ]
}
